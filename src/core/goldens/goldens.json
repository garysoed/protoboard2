hemeDisabledFG1: var(--mkThemeMainDisabledFG1);\n--mkThemeDisabledBG1: var(--mkThemeMainDisabledBG1);\n--mkThemePassiveFG2: var(--mkThemeMainPassiveFG2);\n--mkThemePassiveBG2: var(--mkThemeMainPassiveBG2);\n--mkThemeActionFG2: var(--mkThemeMainActionFG2);\n--mkThemeActionBG2: var(--mkThemeMainActionBG2);\n--mkThemeHoverFG2: var(--mkThemeMainHoverFG2);\n--mkThemeHoverBG2: var(--mkThemeMainHoverBG2);\n--mkThemeFocusFG2: var(--mkThemeMainFocusFG2);\n--mkThemeFocusBG2: var(--mkThemeMainFocusBG2);\n--mkThemeDisabledFG2: var(--mkThemeMainDisabledFG2);\n--mkThemeDisabledBG2: var(--mkThemeMainDisabledBG2); }\n\n[mk-theme-context=\"side\"] { --mkThemePassiveFG1: var(--mkThemeSidePassiveFG1);\n--mkThemePassiveBG1: var(--mkThemeSidePassiveBG1);\n--mkThemeActionFG1: var(--mkThemeSideActionFG1);\n--mkThemeActionBG1: var(--mkThemeSideActionBG1);\n--mkThemeHoverFG1: var(--mkThemeSideHoverFG1);\n--mkThemeHoverBG1: var(--mkThemeSideHoverBG1);\n--mkThemeFocusFG1: var(--mkThemeSideFocusFG1);\n--mkThemeFocusBG1: var(--mkThemeSideFocusBG1);\n--mkThemeDisabledFG1: var(--mkThemeSideDisabledFG1);\n--mkThemeDisabledBG1: var(--mkThemeSideDisabledBG1);\n--mkThemePassiveFG2: var(--mkThemeSidePassiveFG2);\n--mkThemePassiveBG2: var(--mkThemeSidePassiveBG2);\n--mkThemeActionFG2: var(--mkThemeSideActionFG2);\n--mkThemeActionBG2: var(--mkThemeSideActionBG2);\n--mkThemeHoverFG2: var(--mkThemeSideHoverFG2);\n--mkThemeHoverBG2: var(--mkThemeSideHoverBG2);\n--mkThemeFocusFG2: var(--mkThemeSideFocusFG2);\n--mkThemeFocusBG2: var(--mkThemeSideFocusBG2);\n--mkThemeDisabledFG2: var(--mkThemeSideDisabledFG2);\n--mkThemeDisabledBG2: var(--mkThemeSideDisabledBG2); }\n\n[mk-theme-context=\"highlight\"] { --mkThemePassiveFG1: var(--mkThemeHighlightPassiveFG1);\n--mkThemePassiveBG1: var(--mkThemeHighlightPassiveBG1);\n--mkThemeActionFG1: var(--mkThemeHighlightActionFG1);\n--mkThemeActionBG1: var(--mkThemeHighlightActionBG1);\n--mkThemeHoverFG1: var(--mkThemeHighlightHoverFG1);\n--mkThemeHoverBG1: var(--mkThemeHighlightHoverBG1);\n--mkThemeFocusFG1: var(--mkThemeHighlightFocusFG1);\n--mkThemeFocusBG1: var(--mkThemeHighlightFocusBG1);\n--mkThemeDisabledFG1: var(--mkThemeHighlightDisabledFG1);\n--mkThemeDisabledBG1: var(--mkThemeHighlightDisabledBG1);\n--mkThemePassiveFG2: var(--mkThemeHighlightPassiveFG2);\n--mkThemePassiveBG2: var(--mkThemeHighlightPassiveBG2);\n--mkThemeActionFG2: var(--mkThemeHighlightActionFG2);\n--mkThemeActionBG2: var(--mkThemeHighlightActionBG2);\n--mkThemeHoverFG2: var(--mkThemeHighlightHoverFG2);\n--mkThemeHoverBG2: var(--mkThemeHighlightHoverBG2);\n--mkThemeFocusFG2: var(--mkThemeHighlightFocusFG2);\n--mkThemeFocusBG2: var(--mkThemeHighlightFocusBG2);\n--mkThemeDisabledFG2: var(--mkThemeHighlightDisabledFG2);\n--mkThemeDisabledBG2: var(--mkThemeHighlightDisabledBG2); }\n/***********\n * Variables\n ***********/\n * {\n  --mkFontFamilyHeader: 'Montserrat', cursive;\n  --mkFontFamilyNormal: 'Lato', sans-serif;\n  --mkFontFamilyMono: 'Ubuntu Mono', monospace;\n\n  --mkFontWeightNormal: 400;\n  --mkFontWeightBold: 700;\n  --mkLineHeight: 2rem;\n\n  --mkSizePadding: 1rem;\n\n  --mkAnimationTime: .15s;\n\n  --mkGridColumn: 4rem;\n  --mkGridGap: 1rem;\n}\n\n/*****************\n * Theme variables\n *****************/\n[mk-passive-1],\n[mk-theme-context],\n:root {\n  --mkThemeBG: var(--mkThemePassiveBG1);\n  --mkThemeFG: var(--mkThemePassiveFG1);\n}\n\n[mk-passive-2] {\n  --mkThemeBG: var(--mkThemePassiveBG2);\n  --mkThemeFG: var(--mkThemePassiveFG2);\n}\n\n[mk-action-1] {\n  --mkThemeBG: var(--mkThemeActionBG1);\n  --mkThemeFG: var(--mkThemeActionFG1);\n}\n\n[mk-action-2] {\n  --mkThemeBG: var(--mkThemeActionBG2);\n  --mkThemeFG: var(--mkThemeActionFG2);\n}\n\na {\n  --mkThemeBG: var(--mkThemeActionBG2);\n  --mkThemeFG: var(--mkThemeActionFG2);\n}\n\n[mk-action-1]:focus-within {\n  --mkThemeBG: var(--mkThemeFocusBG1);\n  --mkThemeFG: var(--mkThemeFocusFG1);\n}\n\n[mk-action-2]:focus-within {\n  --mkThemeBG: var(--mkThemeFocusBG2);\n  --mkThemeFG: var(--mkThemeFocusFG2);\n}\n\na:focus-within {\n  --mkThemeBG: var(--mkThemeFocusBG2);\n  --mkThemeFG: var(--mkThemeFocusFG2);\n}\n\n[mk-action-1]:hover {\n  --mkThemeBG: var(--mkThemeHoverBG1);\n  --mkThemeFG: var(--mkThemeHoverFG1);\n}\n\n[mk-action-2]:hover {\n  --mkThemeBG: var(--mkThemeHoverBG2);\n  --mkThemeFG: var(--mkThemeHoverFG2);\n}\n\na:hover {\n  --mkThemeBG: var(--mkThemeHoverBG2);\n  --mkThemeFG: var(--mkThemeHoverFG2);\n}\n\n[mk-action-1][mk-disabled] {\n  --mkThemeBG: var(--mkThemeDisabledBG1);\n  --mkThemeFG: var(--mkThemeDisabledFG1);\n}\n\n[mk-action-2][mk-disabled] {\n  --mkThemeBG: var(--mkThemeDisabledBG2);\n  --mkThemeFG: var(--mkThemeDisabledFG2);\n}\n\n/****************\n * Applied themes\n ****************/\n[mk-action-1], [mk-action-2],\n[mk-passive-1], [mk-passive-2],\n[mk-body-1], [mk-body-2],\n[mk-theme-context],\na {\n  background: var(--mkThemeBG);\n  color: var(--mkThemeFG);\n}\n\n/*************\n * Applied CSS\n *************/\n\nhtml, button, input {\n  font-family: var(--mkFontFamilyNormal);\n}\n\nhtml {\n  font-size: 75%;\n}\n\nbody {\n  margin: 0;\n}\n\n[mk-body-1],\n[mk-body-2],\na,\nbutton,\ncode,\npre,\nkbd {\n  font-size: 1.25rem;\n  line-height: 2rem;\n}\n\n[mk-body-2] {\n  font-weight: 700;\n}\n\n[mk-action-1],\n[mk-action-2],\na {\n  font-weight: var(--mkFontWeightBold);\n  text-decoration: none;\n}\n\na > code {\n  color: var(--mkThemeActionFG1);\n}\n\na:hover, a:focus {\n  cursor: pointer;\n  text-decoration: underline;\n}\n\na:hover > code,\na:focus > code {\n  color: var(--mkThemeActionFocusFG1);\n}\n\na:focus {\n  outline: none;\n}\n\nh1 {\n  font-size: 5rem;\n  font-weight: 300;\n  line-height: 8rem;\n}\n\nh2 {\n  font-size: 4rem;\n  font-weight: 400;\n  line-height: 8rem;\n}\n\nh3 {\n  font-size: 3rem;\n  font-weight: 400;\n  line-height: 8rem;\n}\n\nh4 {\n  font-size: 2rem;\n  font-weight: 400;\n  line-height: 4rem;\n}\n\nh5 {\n  font-size: 1.75rem;\n  font-weight: 500;\n  line-height: 4rem;\n}\n\nh6 {\n  font-size: 1.5rem;\n  font-weight: 700;\n  line-height: 4rem;\n}\n\nh1, h2, h3, h4, h5, h6 {\n  color: var(--mkThemeFG);\n  font-family: var(--mkFontFamilyHeader);\n  margin: 0;\n}\n\nhr {\n  margin-block-start: 2rem;\n  margin-block-end: 2rem;\n  border-style: solid;\n  border-bottom-color: var(--mkThemeActionDisabledFG2);\n  border-top: none;\n  border-left: none;\n  border-right: none;\n}\n\ncaption, code, figcaption, pre, kbd {\n  color: var(--mkThemeFG);\n  font-family: var(--mkFontFamilyMono);\n}\n\ncaption, figcaption {\n  font-size: 1rem;\n}\n\nfigure {\n  margin-inline-start: 4rem;\n  margin-inline-end: 4rem;\n}\n\npre {\n  padding: 1rem;\n}\n\n/* fieldset {\n  border: 2px solid var(--mkThemeActionFG1);\n  padding: 1rem 1rem 2rem 1rem;\n}\n\nfieldset:hover, fieldset:focus {\n  border-color: var(--mkThemeActionFocusFG1);\n} */\n\ninput {\n  background-color: var(--mkThemeBG);\n}\n\ninput:-webkit-autofill,\ninput:-webkit-autofill:hover,\ninput:-webkit-autofill:focus,\ntextarea:-webkit-autofill,\ntextarea:-webkit-autofill:hover,\ntextarea:-webkit-autofill:focus,\nselect:-webkit-autofill,\nselect:-webkit-autofill:hover,\nselect:-webkit-autofill:focus {\n  -webkit-text-fill-color: var(--mkThemeFG);\n  transition: background-color 5000s ease-in-out 0s;\n}\n\nlabel {\n  color: var(--mkThemeFG);\n  font-size: 1.25rem;\n  line-height: 2rem;\n}\n\n/* legend {\n  color: var(--mkThemeActionFG1);\n} */\n\n/* fieldset:hover > legend, fieldset:focus > legend {\n  color: var(--mkThemeActionFocusFG1);\n} */\n\np, pre {\n  margin-block-start: 1rem;\n  margin-block-end: 1rem;\n}\n\n/* Table styling */\ntable {\n  border-collapse: collapse;\n  color: var(--mkThemeFG);\n  font-size: var(--gsFontSizeRoot);\n  table-layout: fixed;\n}\n\ntable tr:nth-child(2n) {\n  background-color: var(--mkThemeBG);\n}\n\nth {\n  font-size: 1.5rem;\n  font-weight: 700;\n  text-align: start;\n}\n\ntd, th {\n  border: 1px solid var(--mkThemeFG);\n  padding: 1rem;\n}\n\ntr:first-child th {\n  border-top: none;\n}\n\nth:first-child,\ntd:first-child {\n  border-left: none;\n}\n\nth:last-child,\ntd:last-child {\n  border-right: none;\n}\n\ntr:last-child th,\ntr:last-child td {\n  border-bottom: none;\n}\n\ndl {\n  display: grid;\n  grid-template-columns: auto 1fr;\n  margin-block-start: 1rem;\n  margin-block-end: 1rem;\n}\n\ndt, dd {\n  padding: 1rem;\n}\n\ndt > p,\ndd > p {\n  margin: 0;\n}\n\ndd {\n  margin-inline-start: 0;\n}\n\ndt {\n  font-weight: var(--mkFontWeightBold);\n}\n\n/* Layouts */\n[layout=\"grid\"] {\n  display: grid;\n  grid-column-gap: 1rem;\n  grid-auto-rows: 4rem;\n}\n\n[layout=\"row\"] {\n  display: flex;\n  flex-direction: row;\n}\n\n[layout=\"column\"] {\n  display: flex;\n  flex-direction: column;\n}\n\n[flex] {\n  flex: 1;\n}\n\n[flex-wrap] {\n  flex-wrap: wrap;\n}\n\n[flex-justify=\"flex-start\"] {\n  justify-content: flex-start;\n}\n\n[flex-justify=\"flex-end\"] {\n  justify-content: flex-end;\n}\n\n[flex-justify=\"center\"] {\n  justify-content: center;\n}\n\n[flex-justify=\"space-between\"] {\n  justify-content: space-between;\n}\n\n[flex-justify=\"space-around\"] {\n  justify-content: space-around;\n}\n\n[flex-align=\"flex-start\"] {\n  align-items: flex-start;\n}\n\n[flex-align=\"flex-end\"] {\n  align-items: flex-end;\n}\n\n[flex-align=\"center\"] {\n  align-items: center;\n}\n\n[flex-align=\"stretch\"] {\n  align-items: stretch;\n}\n\n[flex-align=\"baseline\"] {\n  align-items: baseline;\n}\n\n[fill-parent] {\n  bottom: 0;\n  left: 0;\n  position: absolute;\n  right: 0;\n  top: 0;\n}\n\n[shadow=\"up\"] {\n  box-shadow: 0 1px 8px 0 rgba(0,0,0,.2),\n      0 3px 4px 0 rgba(0,0,0,.14),\n      0 3px 3px -2px rgba(0,0,0,.12);\n}\n[shadow=\"down\"] {\n  position: relative;\n}\n[shadow=\"down\"]::before {\n  content: '';\n\n  position: absolute;\n  left: 0px;\n  top: 0px;\n  right: 0px;\n  bottom: 0px;\n\n  box-shadow: inset 4px 1px 8px 0 rgba(0,0,0,.2),\n      inset 0 3px 4px 0 rgba(0,0,0,.14),\n      inset 1.5px 3px 3px -2px rgba(0,0,0,.12);\n  pointer-events: none;\n}\n</style></gst-fake-shadow></pbt-test>",
